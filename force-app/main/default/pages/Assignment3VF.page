<apex:page controller="MyMerchandise" >
    <apex:pageBlock title="Merchandise">
        <apex:pageBlockTable value="{!MerchandiseList}" var="tem">
        <apex:column value="{!tem.Name__c}"/>
            <apex:column value="{!tem.Job_Description__c}"/>
            <apex:column value="{!tem.Price__c}"/>
            <apex:column value="{!tem.Total_Inventory__c}"/>
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>