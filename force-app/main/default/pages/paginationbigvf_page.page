<apex:page controller="Pagination" sidebar="false" showHeader="false"> 
    
<!--*******************************************************************CSS***************************************************************************-->
    <style>
        .alignleft {
        float: left;
        }
        .aligncenter {
        float: left;
        }
        .alignright {
        float: right;
        }
        .pannelConfigs {
        margin-left: 250px;
        }
        .memSpaceSmall{
        margin-left: 50px;
        }
        
        .float_center {
        float: right;
        
        position: relative;
        left: 150%; /* or right 50% */
        text-align: center;
        }
        
        .spinnerBg{
        width: 500%;
        height: 500%;
        position: absolute;
        background-color: #000;
        opacity: 0.2;
        z-index: 999999;
        }
        .spinner{
        width: 100%;
        height: 100%;
        position: absolute;
        background-image: url("/img/loading32.gif");
        background-size: 16px;
        background-repeat: no-repeat;
        background-attachment: fixed;
        background-position: center;
        z-index: 9999999;
        opacity: 1;
        }
<!--*******************************************************************CSS***************************************************************************-->      
    </style>
    
    <apex:form id="b11">
        <apex:pageblock >
            
            <apex:actionStatus id="pageStatus">    
                <apex:facet name="start">
                    <div class="spinnerBg" />
                    <div class="spinner" />
                </apex:facet>
            </apex:actionStatus>
            
<!-- ****************************************************** All Objects Block ***************************************************************************** -->
            <apex:pageBlockSection >
                
                <apex:pageBlockSectionItem >
                    <apex:outputLabel value="Select Object : "  style="font-size:15px;" ></apex:outputLabel>
                    <apex:selectList value="{!secObject}" size="1" multiselect="false"  >
                        <apex:selectOptions value="{!objectList}" />
                        <apex:actionSupport action="{!displayFields}" event="onchange" reRender="b11" status="pageStatus"/>
                    </apex:selectList>
                </apex:pageBlockSectionItem>
                <apex:commandButton action="{!reset}" value="Reset" reRender="b11"  style="width:100px" status="pageStatus" disabled="{!secObject == ''}"/> 
            </apex:pageBlockSection> 
        </apex:pageblock>
        
<!-- ************************************************************ All Objects Block ********************************************************************* -->
        <apex:pageblock rendered="{!piclistblock}">
            {!secObject} :Field                                                                      
            <apex:outputPanel >
                <apex:panelGrid columns="4" > 
                    
<!-- ************************************************************* Available Fields Options ******************************************************************* -->
                    <apex:panelGroup >
                        <apex:selectList value="{!selectedFields}" size="12" multiselect="true" style="Width:200px;height:200px;" styleClass="memSpaceSmall" >
                            <apex:selectOptions value="{!FieldList}" />
                        </apex:selectList>
                    </apex:panelGroup>
                    
                    <apex:panelGroup >
                        <br/>
                        <br/>
                        <br/>
                        <div class="float_center">
                            <apex:commandButton action="{!addButton}" value="Add" reRender="b11" style="width:100px" status="pageStatus" disabled="{!disableAddButton}" />
                            <br/>
                            <br/>
                            <apex:commandButton action="{!removeButton}" value="Remove" reRender="b11"  style="width:100px" status="pageStatus" disabled="{!disableRemoveButton}" />
                        </div>
                    </apex:panelGroup>
                    
                    <apex:panelGroup styleClass="pannelConfigs" >
                        <apex:selectList value="{!showselectedfield}"  size="12" multiselect="true" style="Width:200px;height:200px;" styleClass="memSpaceSmall">
                            <apex:selectOptions value="{!addFields}"/>
                        </apex:selectList> <br/><br/>
                        <div style="text-align:right;">
                            <apex:outputLabel value="Max 10 Fields Allowed to Add at a Time "></apex:outputLabel>
                        </div>
                    </apex:panelGroup>
                    
                    <apex:panelGroup >
                        <br/>
                        <br/>    
                        <apex:commandButton action="{!processButton}" value="Process Records" oncomplete="b1()"  style="width:150px" status="pageStatus" disabled="{!disableRemoveButton}" reRender="b11"/>
                    </apex:panelGroup>
                    
<!-- ************************************************************* Available Fields Options **************************************************************************************** -->
                </apex:panelGrid>
            </apex:outputPanel>
        </apex:pageblock>  
        
<!-- *************************************************************** Pagination Component ***************************************************************************** -->
        <c:paginationComponent button="{!secObject}" Field="{!addFields}" rendered="{!renderChildComponent}"></c:paginationComponent>        
    </apex:form>
</apex:page>